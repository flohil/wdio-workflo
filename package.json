{
  "name": "wdio-workflo",
  "version": "0.0.138",
  "description": "Framework for automated functional system tests in gherkin style",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "webdriverio-workflo ./config/wdio.conf.js --color",
    "generate_report": "node ./scripts/generate_allure_report.js",
    "open_report": "node ./scripts/open_allure_report.js",
    "latest_report": "npm run generate_report && npm run open_report",
    "start": "npm run build:live",
    "build:live": "nodemon --exec ts-node -- ./src/lib/testElementsAndStores.ts",
    "prepublish": "npm run build"
  },
  "author": "Florian Hilbinger",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/flohil/wdio-workflo"
  },
  "bugs": {
    "url": "https://github.com/flohil/wdio-workflo/issues"
  },
  "devDependencies": {},
  "dependencies": {
    "@types/jasmine": "^2.5.52",
    "@types/jasmine-expect": "^3.6.1",
    "@types/lodash": "^4.14.68",
    "@types/node": "^8.0.4",
    "@types/webdriverio": "^4.7.1",
    "allure-commandline": "2.1.1",
    "circular-json": "^0.3.3",
    "colors": "^1.1.2",
    "deep-diff": "^0.3.8",
    "deep-freeze": "0.0.1",
    "get-installed-path": "^2.0.3",
    "gulp": "^3.9.1",
    "gulp-shell": "^0.6.3",
    "gulp-typescript": "^3.1.7",
    "gulp-webserver": "^0.9.1",
    "gutil": "^1.6.4",
    "htmlparser2": "^3.9.2",
    "jasmine-expect": "^3.7.0",
    "jasmine-object-matchers-jest": "^0.1.0",
    "jasmine-reporters": "^2.2.1",
    "jasmine2-custom-message": "^0.8.2",
    "jsonfile": "^3.0.1",
    "lodash": "^4.17.4",
    "nodemon": "^1.11.0",
    "path": "^0.12.7",
    "tcp-port-used": "^0.1.2",
    "ts-node": "3.2.0",
    "tsconfig-paths": "^2.2.0",
    "typescript": "^2.4.2",
    "wdio-selenium-standalone-service": "0.0.9",
    "wdio-workflo-allure-reporter": "0.0.15",
    "wdio-workflo-jasmine-framework": "0.0.3",
    "wdio-workflo-spec-reporter": "0.0.9",
    "webdriverio-workflo": "0.0.23",
    "wrench": "^1.5.9"
  }
}
