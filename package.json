{
  "name": "workflo",
  "version": "0.0.1",
  "description": "Framework for automated functional system tests",
  "main": "index.js",
  "scripts": {
    "test": "wdio ./config/wdio.conf.js --color",
    "generate_report": "allure generate ./testDir/results/$(cat ./testDir/results/latestRun)/allure-results/ -o ./testDir/results/$(cat ./testDir/results/latestRun)/allure-report",
    "latest_report": "npm run generate_report && allure report open -o ./testDir/results/$(cat ./testDir/results/latestRun)/allure-report",
    "start": "npm run build:live",
    "build:live": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/experimental/specs.ts"
  },
  "author": "Florian Hilbinger",
  "license": "ISC",
  "devDependencies": {
    "@types/jasmine": "^2.5.52",
    "@types/jasmine-expect": "^3.6.1",
    "@types/webdriverio": "^4.7.1",
    "axios": "^0.16.2",
    "axios-mock-adapter": "^1.8.1",
    "colors": "^1.1.2",
    "deep-diff": "^0.3.8",
    "deep-freeze": "0.0.1",
    "gulp": "^3.9.1",
    "gulp-shell": "^0.6.3",
    "gulp-typescript": "^3.1.7",
    "gulp-webserver": "^0.9.1",
    "gutil": "^1.6.4",
    "jasmine-allure-reporter": "^1.0.1",
    "jasmine-expect": "^3.7.0",
    "jasmine-object-matchers-jest": "^0.1.0",
    "jasmine-reporters": "^2.2.1",
    "jasmine2-custom-message": "^0.8.2",
    "lodash": "^4.17.4",
    "najax": "^1.0.3",
    "nodemon": "^1.11.0",
    "path": "^0.12.7",
    "selenium-standalone": "^6.5.0",
    "tcp-port-used": "^0.1.2",
    "ts-node": "^3.0.6",
    "typescript": "^2.3.4",
    "wdio-allure-addons-reporter": "0.0.4",
    "wdio-allure-reporter": "^0.1.2",
    "wdio-jasmine-framework": "^0.3.1",
    "wdio-junit-reporter": "^0.3.0",
    "wdio-selenium-standalone-service": "0.0.9",
    "wdio-spec-reporter": "^0.1.0",
    "webdriverio": "^4.8.0"
  },
  "dependencies": {}
}
